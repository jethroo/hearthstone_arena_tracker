<h2>My matches</h2>

<div ng-controller="MatchesController">
  <div id="fixed" style="position: absolute; height: 80%; overflow-y: scroll;" when-scrolled="matchesFactory.loadMore()">
    <div class="small-12 medium-6 large-4 columns match_result_box" ng-repeat='match in matches'>
      <%= render partial: "match" %>
    </div>
  </div>
</div>
